CC = @CC@
CFLAGS = -I. @CPPFLAGS@ @CFLAGS@ -Wno-error
LDFLAGS = @LDFLAGS@
top_builddir = @top_builddir@
libdir = @libdir@
LIBTOOL = @LIBTOOL@

PROTO_OBJS = \
	protobuf-c.o

PROTO_HEADERS = \
	protobuf-c.h

all: @PROTOBUFC_LIB@

@PROTOBUFC_LIB@: $(PROTO_OBJS)
	ar rs @PROTOBUFC_LIB@ $(PROTO_OBJS)

$(PROTO_OBJS): %.o: %.c $(PROTO_HEADERS)
	$(CC) $(CFLAGS) -c -o $@ $<

clean:
	rm -f @PROTOBUFC_LIB@ $(PROTO_OBJS)

distclean: clean
	rm -f Makefile


install:

uninstall:

check:

.PHONY: clean distclean install uninstall check
